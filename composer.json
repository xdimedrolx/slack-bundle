{
    "name": "nexylan/slack-bundle",
    "description": "Symfony bundle integration of maknz/slack library",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sullivan SENECHAL",
            "email": "soullivaneuh@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "maknz/slack": "^1.7",
        "symfony/http-kernel": "^2.8 || ^3.0",
        "phpunit/phpunit": "^5.2"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^0.7.6",
        "satooshi/php-coveralls": "^1.0",
        "codeclimate/php-test-reporter": "~0.3",
        "sllh/php-cs-fixer-styleci-bridge": "^1.5"
    },
    "autoload": {
        "psr-4": { "Nexy\\SlackBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Nexy\\SlackBundle\\Tests\\": "tests" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
